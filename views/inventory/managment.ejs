<% if (title) { %>
    <h1> <%= title %> </h1>
    <p><a href="/inv/addClassification">Add New Classification</a></p>
    <p><a href="/inv/addInventory">Add New Inventory</a></p>
    <h2>Manage Vehicles</h2>
    <p>Select an item from the list.</p>
    <%- dropmenu %>
    <table id="inventoryDisplay"></table>
    <% } else {
      res.redirect('/')
    } %>

<% if (flash && flash.message) { %>
    <div class="flash-message">
        <%= flash.message %>
    </div>
<% } %>


